.submit-p{ padding-top:18px; border-top:1px solid #d1d1d1; height:30px;}
.submit-p input{ width:63px; height:30px; font-weight:bold; color:#fff; text-align:center; border-radius:2px; border:none;}
.submit-p .submitbutton{ background:#2c3d4f; margin-left:297px;}
.submit-p .submitbutton:hover{ background:#182e45;}
.submit-p .resetbutton{ background:#f4a121; margin-left:10px;}
.submit-p .resetbutton:hover{background:#d98b13;}

/*网站地图*/
.map-t{font-family:\5FAE\8F6F\96C5\9ED1,sans-serif; font-weight:normal; font-size:22px; color:#282828; height:78px; line-height:78px;border-bottom:1px solid #d1d1d1;}
.map-stafflist{ padding-top:42px;}
.map-stafflist dd{width:326px; float:left; border-right:1px dotted #d1d1d1;}
.map-stafflist dd.dd-1{width:329px;}
.map-stafflist dd.dd-2{width:335px;}
.map-stafflist dd.dd-3{ width:314px; border-right:none;}
.map-stafflist h3{font-family:\5FAE\8F6F\96C5\9ED1,sans-serif; font-weight:normal; font-size:18px; color:#282828; height:24px; line-height:24px; color:#01599b; text-align:center; text-indent:-40px;}
.map-stafflist .list{width:260px; overflow:hidden; margin:17px auto 0px auto;}
.map-stafflist .list li{line-height:30px; line-height:30px; float:left; width:115px; padding-left:15px; background:url("otherbg.png")/*tpa=http://its.tsinghua.edu.cn/yhfwpt/images/otherbg.png*/ no-repeat; background-position: 0px -78px;}

.map-innercom{ font-family:\5FAE\8F6F\96C5\9ED1,sans-serif; font-weight:normal; font-size:18px; color:#282828; height:74px; line-height:74px; color:#01599b; text-align:center; margin-top:58px; border-top:1px solid #d1d1d1;}
.map-inner {margin-top:13px;}
.map-inner li{ float:left; width:189px; border-right:1px dotted #d1d1d1; padding-left:55px;min-height:160px;}
.map-inner li h2{height:38px; line-height:38px;font-weight:normal; font-size:16px; color:#282828;font-family:\5FAE\8F6F\96C5\9ED1,sans-serif; color:#262626;}
.map-inner li .list{ padding-left:15px; background:url("../images/otherbg.png")/*tpa=http://its.tsinghua.edu.cn/yhfwpt/images/otherbg.png*/ no-repeat; background-position: 0px -78px; height:30px; line-height:30px;}
.map-inner li .list a{ color:#353535;}
.map-inner li .list a:hover{color:#0093d0;}
